    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/11/Owner"/>
    


    <!-- http://www.w3.org/ns/prov#Activity -->

    <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity">
        <rdfs:label xml:lang="en">Activity</rdfs:label>
        <owl:sameAs>http://dbpedia.org/ontology/Activity</owl:sameAs>
        <skos:definition xml:lang="en">An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities.</skos:definition>
    </owl:Class>
    


