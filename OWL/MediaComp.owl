    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/Broadcaster -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/Broadcaster">
        <rdfs:subClassOf rdf:resource="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany"/>
        <skos:definition xml:lang="en">A company that broadcasts audio or video content to a dispersed audience via any audio visual medium.</skos:definition>
        <rdfs:label xml:lang="en">Broadcaster</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#Broadcaster</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany">
        <skos:definition xml:lang="en">A company which deals with media products and material</skos:definition>
        <rdfs:label xml:lang="fr">Compagnie multim√©dia</rdfs:label>
        <rdfs:label xml:lang="en">Media company</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#MediaCompany</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MovieCompany -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MovieCompany">
        <rdfs:subClassOf rdf:resource="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany"/>
        <skos:definition xml:lang="en">A comapny making films, also called movies or motion pictures, is a series of still or moving images</skos:definition>
        <rdfs:label xml:lang="en">Movie company</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#Movie</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MusicalProductor -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MusicalProductor">
        <rdfs:subClassOf rdf:resource="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany"/>
        <rdfs:label xml:lang="en">Musical productor</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#MusicalProductor</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/NewsAgency -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/NewsAgency">
        <rdfs:subClassOf rdf:resource="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany"/>
        <skos:definition xml:lang="en">A news agency is an organization of journalists established to supply news reports to news organizations: newspapers, magazines, and radio and television broadcasters.</skos:definition>
        <rdfs:label xml:lang="en">News agency</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#NewsAgency</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/PublishingCompany -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/PublishingCompany">
        <rdfs:subClassOf rdf:resource="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/MediaCompany"/>
        <skos:definition xml:lang="en">An organization which main activity is mass publishing of information. Often, the publishers issue periodic and/or non-periodic documents on paper media. While on-line publishing is considered a typical activity for publishers, the streaming electronic media such as TV and Radio Stations are not.</skos:definition>
        <rdfs:label xml:lang="en">Publishing company</rdfs:label>
        <owl:sameAs>http://www.ontotext.com/proton/protonext#PublishingCompany</owl:sameAs>
    </owl:Class>
    


