    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/11/MediaClient -->

    <owl:Class rdf:about="http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/11/MediaClient">
        <skos:definition xml:lang="en">A mass media client or consumer (for instance an advertising company) abstracted from its organizational and ownership aspects and considered more as a social phenomenon, partly a brand.</skos:definition>
        <rdfs:label xml:lang="en">Media Client</rdfs:label>
    </owl:Class>
    


    <!-- http://www.OntologyTestChallenge.org/hacenecherfi/ontologies/2018/11/OwnerOf -->

